          xml += '<p class="s1" style="padding-top: 1pt;padding-left: 5pt;text-indent: 0pt;text-align: left;">DATE OF SALE</p>';
          xml += '<p class="s2" style="padding-top: 0pt;padding-left: 0pt;text-indent: 0pt;text-align: left;">' + shipDate + '</p>';
          xml += '</td>';
